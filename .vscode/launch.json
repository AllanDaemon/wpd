	"version": "0.2.0",
	"configurations": [

		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: module apod",
			"type": "python",
			"request": "launch",
			"module": "providers.apod",
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: gui",
			"type": "python",
			"request": "launch",
			"module": "gui",
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal"
		}
	]
}